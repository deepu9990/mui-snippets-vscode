      "    backgroundColor: 'primary.dark',",
      "  },}",
      "  ${8:[theme.breakpoints.down('md')]: {",
      "    p: 1,",
      "  },}",
      "}}"
    ],
    "description": "Material UI sx prop with common styling patterns"
  },
  "muiColorMode": {
    "prefix": ["muiColorMode", "mui_color_mode", "muicolormode"],
    "body": [
      "import { useColorScheme } from \"@mui/material/styles\";",
      "import { Button } from \"@mui/material\";",
      "",
      "function ColorModeToggle() {",
      "  const { mode, setMode } = useColorScheme();",
      "",
      "  return (",
      "    <Button",
      "      onClick={() => {",
      "        setMode(mode === 'light' ? 'dark' : 'light');",
      "      }}",
      "    >",
      "      {mode === 'light' ? '${1:Dark}' : '${2:Light}'} mode",
      "    </Button>",
      "  );",
      "}"
    ],
    "description": "Material UI color scheme toggle using useColorScheme"
  },

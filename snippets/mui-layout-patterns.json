{
  "muiAppBarWithDrawer": {
    "prefix": ["muiAppBarWithDrawer", "mui_app_bar_with_drawer", "muiappbarwithdrawer"],
    "body": [
      "import { AppBar, Toolbar, Typography, IconButton, Drawer, List, ListItem, ListItemButton, ListItemText } from \"@mui/material\";",
      "import { Menu as MenuIcon } from \"@mui/icons-material\";",
      "import { useState } from \"react\";",
      "",
      "const [drawerOpen, setDrawerOpen] = useState(false);",
      "",
      "const toggleDrawer = (open) => (event) => {",
      "  if (event.type === 'keydown' && (event.key === 'Tab' || event.key === 'Shift')) {",
      "    return;",
      "  }",
      "  setDrawerOpen(open);",
      "};",
      "",
      "<>",
      "  <AppBar position=\"static\">",
      "    <Toolbar>",
      "      <IconButton",
      "        size=\"large\"",
      "        edge=\"start\"",
      "        color=\"inherit\"",
      "        aria-label=\"menu\"",
      "        sx={{ mr: 2 }}",
      "        onClick={toggleDrawer(true)}",
      "      >",
      "        <MenuIcon />",
      "      </IconButton>",
      "      <Typography variant=\"h6\" component=\"div\" sx={{ flexGrow: 1 }}>",
      "        ${1:App Title}",
      "      </Typography>",
      "    </Toolbar>",
      "  </AppBar>",
      "  <Drawer anchor=\"left\" open={drawerOpen} onClose={toggleDrawer(false)}>",
      "    <List sx={{ width: 250 }}>",
      "      {${2:['Home', 'About', 'Services', 'Contact']}.map((text) => (",
      "        <ListItem key={text} disablePadding>",
      "          <ListItemButton onClick={toggleDrawer(false)}>",
      "            <ListItemText primary={text} />",
      "          </ListItemButton>",
      "        </ListItem>",
      "      ))}",
      "    </List>",
      "  </Drawer>",
      "</>"
    ],
    "description": "Material UI AppBar with responsive Drawer navigation"
  },
  "muiResponsiveAppBar": {
    "prefix": ["muiResponsiveAppBar", "mui_responsive_app_bar", "muiresponsiveappbar"],
    "body": [
      "import { AppBar, Toolbar, Typography, IconButton, Menu, MenuItem, Button, Box } from \"@mui/material\";",
      "import { Menu as MenuIcon, AccountCircle } from \"@mui/icons-material\";",
      "import { useState } from \"react\";",
      "",
      "const [anchorEl, setAnchorEl] = useState(null);",
      "",
      "const handleMenu = (event) => {",
      "  setAnchorEl(event.currentTarget);",
      "};",
      "",
      "const handleClose = () => {",
      "  setAnchorEl(null);",
      "};",
      "",
      "<AppBar position=\"static\">",
      "  <Toolbar>",
      "    <IconButton",
      "      size=\"large\"",
      "      edge=\"start\"",
      "      color=\"inherit\"",
      "      aria-label=\"menu\"",
      "      sx={{ mr: 2, display: { md: 'none' } }}",
      "    >",
      "      <MenuIcon />",
      "    </IconButton>",
      "    <Typography variant=\"h6\" component=\"div\" sx={{ flexGrow: 1 }}>",
      "      ${1:App Name}",
      "    </Typography>",
      "    <Box sx={{ display: { xs: 'none', md: 'flex' } }}>",
      "      <Button color=\"inherit\">${2:Home}</Button>",
      "      <Button color=\"inherit\">${3:About}</Button>",
      "      <Button color=\"inherit\">${4:Services}</Button>",
      "    </Box>",
      "    <IconButton",
      "      size=\"large\"",
      "      aria-label=\"account of current user\"",
      "      aria-controls=\"menu-appbar\"",
      "      aria-haspopup=\"true\"",
      "      onClick={handleMenu}",
      "      color=\"inherit\"",
      "    >",
      "      <AccountCircle />",
      "    </IconButton>",
      "    <Menu",
      "      id=\"menu-appbar\"",
      "      anchorEl={anchorEl}",
      "      anchorOrigin={{",
      "        vertical: 'top',",
      "        horizontal: 'right',",
      "      }}",
      "      keepMounted",
      "      transformOrigin={{",
      "        vertical: 'top',",
      "        horizontal: 'right',",
      "      }}",
      "      open={Boolean(anchorEl)}",
      "      onClose={handleClose}",
      "    >",
      "      <MenuItem onClick={handleClose}>${5:Profile}</MenuItem>",
      "      <MenuItem onClick={handleClose}>${6:My Account}</MenuItem>",
      "      <MenuItem onClick={handleClose}>${7:Logout}</MenuItem>",
      "    </Menu>",
      "  </Toolbar>",
      "</AppBar>"
    ],
    "description": "Responsive Material UI AppBar with mobile menu"
  },
